    "expo-status-bar": "~1.4.0",
    "graphql": "^15.8.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-native": "0.69.6",
    "react-native-splash-screen": "^3.3.0",
    "react-native-web": "~0.18.7"
  },
